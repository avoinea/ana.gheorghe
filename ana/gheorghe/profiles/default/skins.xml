<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Ana Gheorghe">

 <object name="ana_gheorghe_custom_images"
    meta_type="Filesystem Directory View"
    directory="ana.gheorghe:skins/ana_gheorghe_custom_images"/>
 <object name="ana_gheorghe_custom_templates"
    meta_type="Filesystem Directory View"
    directory="ana.gheorghe:skins/ana_gheorghe_custom_templates"/>
 <object name="ana_gheorghe_styles"
    meta_type="Filesystem Directory View"
    directory="ana.gheorghe:skins/ana_gheorghe_styles"/>

 <skin-path name="Ana Gheorghe" based-on="Plone Default">
  <layer name="ana_gheorghe_custom_images"
     insert-after="custom"/>
  <layer name="ana_gheorghe_custom_templates"
     insert-after="ana_gheorghe_custom_images"/>
  <layer name="ana_gheorghe_styles"
     insert-after="ana_gheorghe_custom_templates"/>
 </skin-path>

</object>
